  "files": [
    "dist/index.cjs",
    "dist/index.mjs",
    "dist/index.d.ts",
    "dist/types.d.ts",
    "dist/render.node.mjs",
    "dist/render.browser.js",
    "dist/render.wasm",
    "dist/renderSync.js",
    "dist/renderSync.d.ts",
    "sync/index.js",
    "sync/index.d.ts",
    "wasm",
    "worker"
  ],
  "contributors": [
